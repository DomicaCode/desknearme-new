{% parse_json object %}
  {
    "item_inventory_id": {{ object.item_inventory_id | json }},
    "uuid":        {{ object.uuid | json }},
    "currency":    {{ object.currency | json }},
    "price":       {{ object.price | plus: 0 | amount_to_fractional: object.currency | to_positive_integer: 1 }},
    "return_days": {{ object.return_days | plus: 0 | json }},
    "tax_rate":    {{ object.tax_rate | json }},
    "category":    {{ object.category | default: "" | json }},
    "lang":        {{ object.lang | default: "en" | json }},
    "name":        {{ object.name | json }},
    "description": {{ object.description | default: "" | json }},
    "tags":        {{ object.tags | downcase | split: ',' | uniq }},
    "location_name": {{ object.location_name | default: "" | json }},
    "location_address": {{ object.location_address | default: "" | json }},
    "zip_code": {{ object.zip_code | default: "" | json }},
    "location_type": {{ object.location_type | default: "" | json }},
    "quantity": {{ object.quantity | plus: 0 | times: 100 | to_positive_integer: 1 }},
    "availability": {{ object.availability | default: "" | json }},
    "property_status": {{ object.property_status | default: "" | json }},
    "desk_type": {{ object.desk_type | default: "" | json }},
    "price_per_hour": {{ object.price_per_hour | to_positive_integer: 0 }},
    "price_per_day": {{ object.price_per_day | to_positive_integer: 0 }},
    "price_per_work_week": {{ object.price_per_work_week | to_positive_integer: 0 }},
    "price_per_calender_week": {{ object.price_per_calender_week | to_positive_integer: 0 }},
    "price_per_month": {{ object.price_per_month | to_positive_integer: 0 }}
  }
{% endparse_json %}

{% liquid
  return object
%}
