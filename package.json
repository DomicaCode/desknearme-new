{
  "scripts": {
    "dev-watch": "npx cross-env NODE_ENV=development webpack-cli -w",
    "dev-gui": "pos-cli gui serve dev",
    "dev-sync": "pos-cli sync dev --livereload",
    "dev-logs": "pos-cli logs dev",
    "start": "npx concurrently --kill-others-on-fail npm:dev-*",
    "build": "npx cross-env NODE_ENV=production webpack-cli",
    "build:dev": "npx cross-env NODE_ENV=development webpack-cli",
    "test": "testcafe 'chromium:headless --sandbox' --stop-on-first-fail test --reporter html:test-report.html -s path=test/screenshots,fullPage=true,takeOnFails=true --selector-timeout 30000",
    "clean": "npx rimraf app/assets/{js,css}/*",
    "deploy": "npm run build && CI=true pos-cli deploy",
    "predeploy": "npm run clean",
    "ci:build": "docker run -it -u $(id -u ${USER}):$(id -g ${USER}) --rm -v $PWD:/app -w /app node:12-alpine npm ci && npm run build",
    "ci:test": "docker run -it -e MPKIT_URL=$MPKIT_URL -u $(id -u ${USER}):$(id -g ${USER}) --rm -v $PWD:/app -w /app platformos/testcafe \"npm run test\"",
    "ci:test:unit": "sh scripts/run-unit-tests.sh",
    "ci:test:publish": "sh scripts/upload-test-report.sh",
    "postinstall": "npm i --prefix scaffold"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.3.3",
    "@tailwindcss/typography": "^0.4.1",
    "autoprefixer": "^10.2.6",
    "concurrently": "^6.2.0",
    "css-loader": "^5.2.6",
    "esbuild-loader": "^2.13.1",
    "mini-css-extract-plugin": "^1.6.2",
    "postcss": "^8.3.5",
    "postcss-fixes": "^2.0.1",
    "postcss-import": "^14.0.2",
    "postcss-loader": "^6.1.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.2.4",
    "tailwindcss-rtl": "^0.7.3",
    "webpack": "^5.41.0",
    "webpack-cli": "^4.7.2",
    "webpack-require-from": "^1.8.6"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  },
  "browserslist": "> 1% or last 2 years",
  "name": "marketplace-template-poc",
  "version": "1.0.0",
  "description": "eCommerce MVP POC",
  "author": "platformOS",
  "license": "ISC",
  "repository": "mdyd-dev/marketplace-template-poc",
  "dependencies": {
    "@uppy/aws-s3": "^1.7.11",
    "@uppy/core": "^1.19.1",
    "@uppy/dashboard": "^1.20.1",
    "@uppy/drag-drop": "^1.4.29",
    "@uppy/webcam": "^1.8.11",
    "@yaireo/tagify": "^4.3.1",
    "alpinejs": "^2.8.2",
    "easymde": "^2.15.0",
    "highlight.js": "^10.7.2",
    "lodash.debounce": "^4.0.8",
    "timeago.js": "^4.0.2",
    "uuid": "^8.3.2"
  }
}
